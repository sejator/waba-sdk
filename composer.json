{
  "name": "sejator/waba-sdk",
  "description": "Laravel SDK for WhatsApp Cloud API (WABA)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sejator Dev",
      "email": "sejatordev@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Sejator\\WabaSdk\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sejator\\WabaSdk\\WabaServiceProvider"
      ],
      "aliases": {
        "Waba": "Sejator\\WabaSdk\\Facades\\Waba"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
